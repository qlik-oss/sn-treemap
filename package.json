{
  "name": "@nebula.js/sn-treemap",
  "version": "1.0.0-alpha.12",
  "description": "Treemap supernova that is DOM independent",
  "main": "dist/sn-treemap.js",
  "author": "Qlik",
  "license": "MIT",
  "files": [
    "src",
    "index.js",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/node_modules",
    "!jest.config.js",
    "!babel.config.js",
    "!example/**"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "repository": {
    "url": "git@github.com:qlik-oss/sn-treemap.git"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.15.0",
    "@babel/eslint-parser": "7.19.1",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.18.6",
    "@nebula.js/cli": "^3.0.0-rc.2",
    "@nebula.js/cli-build": "^3.0.0-rc.2",
    "@nebula.js/cli-sense": "^3.0.0-rc.2",
    "@nebula.js/cli-serve": "^3.0.0-rc.2",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "babel-jest": "^27.1.0",
    "babel-plugin-styled-components": "^2.0.7",
    "color-parse": "1.4.2",
    "color-rgba": "^2.2.3",
    "color-scales": "3.0.2",
    "commitlint": "^11.0.0",
    "d3": "6.2.0",
    "d3-color": "^3.1.0",
    "d3-hierarchy": "3.1.2",
    "d3-interpolate": "2.0.1",
    "d3-path": "2.0.0",
    "eslint": "8.24.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "27.0.4",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.8",
    "eslint-plugin-react-hooks": "4.6.0",
    "extend": "^3.0.2",
    "hammerjs": "^2.0.8",
    "husky": "^4.2.5",
    "jest": "^27.1.0",
    "picasso-plugin-hammer": "^1.6.5",
    "picasso-plugin-q": "^1.2.1",
    "picasso.js": "^1.2.1",
    "prettier": "2.7.1",
    "qlik-chart-modules": "^0.36.0",
    "qlik-object-conversion": "0.14.0",
    "typescript": "^4.1.3"
  },
  "watch": {
    "dev": {
      "patterns": [
        "src"
      ],
      "extensions": "js, jsx"
    }
  },
  "scripts": {
    "test": "jest",
    "lint": "eslint src",
    "ci:install": "yarn install --frozen-lockfile",
    "build": "node ./tools/build.js --core --ext",
    "build:dev": "node ./tools/build.js --core --ext --mode development",
    "build:watch": "node ./tools/build.js --mode development -w",
    "build:rn": "node ./tools/build.js --reactNative",
    "build:rn:dev": "ditto src ./react-native/dist && rm -rf ./react-native/dist/__test__ && mv ./react-native/dist/index.js ./react-native/dist/sn-treemap.js",
    "start": "nebula serve --build false --type treemap"
  },
  "dependencies": {
    "fs-extra": "^10.1.0",
    "yargs": "^17.5.1"
  },
  "peerDependencies": {
    "@nebula.js/stardust": ">=1.0.0"
  }
}
